%h1 Staff sign in 
= render partial: "supportilla/shared/flash", locals: {flash: flash}
= form_tag signin_path, class: "well" do 
  = label_tag :username 
  = text_field_tag :username, nil, maxlength: 15, required: true
  = label_tag :password 
  = password_field_tag :password, nil, required: true
  = submit_tag "Sign in", class: "btn"

